# Datalab API Configuration
DATALAB_API_KEY=your_datalab_api_key_here
DATALAB_API_URL=https://api.datalab.to/v1/convert

# Anthropic API for LLM prompt processing
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Layout detection service
LAYOUT_PARSER_ENDPOINT=http://localhost:8080

# Processing options
DEFAULT_DPI=400
MAX_CONCURRENT_PAGES=4
ENABLE_OCR_FALLBACK=false

# Output options
OUTPUT_DIR=./output
GENERATE_AUDIT_HTML=true
SAVE_INTERMEDIATE_JSON=true
